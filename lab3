//1)
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
    vector<int>v;
    int a,b;
    cin>>b;
    cout<<"---------------------------------"<<endl;
    for(int i=0;i<b;i++){
        cin>>a;
        v.push_back(a);
    }
    sort(v.begin(),v.end());
    auto s = unique(v.begin(), v.end());
    v.erase(s, v.end());
    cout<<"---------------------------------"<<endl;
    cout<<v.size();
}

